import java.util.Scanner;
class Main{
    public static void main(String[]args){
        int a;
        Scanner sc=new Scanner(System.in);
        a=sc.nextInt();
            if(a>0){
        System.out.println("positive number: ");
    }
    else if(a==0)
    {
        System.out.println("zero: ");
    }
    else
    {
        System.out.println("negative number: ");
    }
}
}
